{
  "name": "black Gmail",
  "author": "catdance124",
  "description": "This extension darkens the background color of Gmail.",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/mail/*"],
      "css": ["style.css"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  }
}